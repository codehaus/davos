    <sequence>
      <element ref="tns:a"/>
    </sequence>
    <attribute name="node" type="string"/>
  </complexType>
</element>

<element name="a">
  <complexType>
    <sequence>
      <element name="b">
        <complexType>
          <attribute name="name" type="string"/>
        </complexType>
      </element>
      <element name="c">
        <complexType>
          <attribute name="name" type="string"/>
        </complexType>
      </element>
    </sequence>
    <attribute name="atr" type="int"/>
  </complexType>
</element>

</schema>
